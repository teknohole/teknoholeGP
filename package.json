{
  "name": "@teknohole/teknohole",
  "version": "1.2.2",
  "description": "Package resmi teknohole.com - SDK untuk WebStorage upload dan manajemen file",
  "keywords": [
    "teknohole",
    "storage",
    "upload",
    "cdn",
    "file-upload",
    "webstorage",
    "cloud-storage"
  ],
  "homepage": "https://github.com/teknohole/teknoholeGP#readme",
  "bugs": {
    "url": "https://github.com/teknohole/teknoholeGP/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teknohole/teknoholeGP.git"
  },
  "license": "MIT",
  "author": "Teknohole <support@teknohole.com>",
  "type": "module",
  "main": "./dist/node.js",
  "module": "./dist/node.js",
  "browser": "./dist/browser.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": {
        "import": "./dist/node.js",
        "require": "./dist/node.cjs"
      },
      "browser": {
        "import": "./dist/browser.js"
      },
      "import": "./dist/node.js",
      "require": "./dist/node.cjs",
      "default": "./dist/node.js"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.js",
      "require": "./dist/node.cjs",
      "default": "./dist/node.js"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "import": "./dist/browser.js",
      "default": "./dist/browser.js"
    },
    "./common": {
      "types": "./dist/common.d.ts",
      "import": "./dist/common.js",
      "default": "./dist/common.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "node scripts/build.js",
    "build:rollup": "rollup -c rollup.config.js",
    "prebuild": "rimraf dist",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.js\""
  },
  "dependencies": {
    "axios": "^1.7.2",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "rollup": "^4.9.6"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
